<div class="row center-xs">
  <div class="col-xs-8 col-md-6">
    <div class="box text-left"> 

      <p id="notice"><%= notice %></p>

      <h1>Proceso del proyecto</h1>

      <table>
        <thead>
          <tr>
            <th>Tiempo real</th>
            <th>Tiempo estimado</th>
            <th>Porcentaje realizado</th>
            <th>Estado</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @project_steps.each do |project_step| %>
            <tr>
              <td><%= project_step.n_realtime %></td>
              <td><%= project_step.n_estimatedTime %></td>
              <td><%= project_step.n_completionPercentage %></td>
              <td><%= project_step.t_state %></td>
              <td><%= link_to 'Ver', project_step %></td>
              <td><%= link_to 'Editar', edit_project_step_path(project_step) %></td>
              <td><%= link_to 'Eliminar', project_step, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>

      <br>

      <%= link_to 'Nuevo', new_project_step_path %>

    </div>    
  </div>
</div>
