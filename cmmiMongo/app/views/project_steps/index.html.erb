<p id="notice"><%= notice %></p>

<h1>Listing Project Steps</h1>

<table>
  <thead>
    <tr>
      <th>N realtime</th>
      <th>N estimatedtime</th>
      <th>N completionpercentage</th>
      <th>T state</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_steps.each do |project_step| %>
      <tr>
        <td><%= project_step.n_realtime %></td>
        <td><%= project_step.n_estimatedTime %></td>
        <td><%= project_step.n_completionPercentage %></td>
        <td><%= project_step.t_state %></td>
        <td><%= link_to 'Show', project_step %></td>
        <td><%= link_to 'Edit', edit_project_step_path(project_step) %></td>
        <td><%= link_to 'Destroy', project_step, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project step', new_project_step_path %>
