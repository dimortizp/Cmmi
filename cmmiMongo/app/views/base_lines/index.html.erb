<p id="notice"><%= notice %></p>

<h1>Lineas base del proyecto</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Fecha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @base_lines.each do |base_line| %>
      <tr>
        <td><%= base_line.t_name %></td>
        <td><%= base_line.n_date %></td>
        <td><%= link_to 'Ver', base_line %></td>
        <td><%= link_to 'Editar', edit_base_line_path(base_line) %></td>
        <td><%= link_to 'Elimar', base_line, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nueva linea base', new_base_line_path %>
