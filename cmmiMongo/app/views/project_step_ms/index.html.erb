<p id="notice"><%= notice %></p>

<h1>Listing Project Step Ms</h1>

<table>
  <thead>
    <tr>
      <th>N step</th>
      <th>N project</th>
      <th>T document</th>
      <th>T type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_step_ms.each do |project_step_m| %>
      <tr>
        <td><%= project_step_m.n_step %></td>
        <td><%= project_step_m.n_project %></td>
        <td><%= project_step_m.t_document %></td>
        <td><%= project_step_m.t_type %></td>
        <td><%= link_to 'Show', project_step_m %></td>
        <td><%= link_to 'Edit', edit_project_step_m_path(project_step_m) %></td>
        <td><%= link_to 'Destroy', project_step_m, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

